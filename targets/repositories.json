{
    "repositories": {
        "mohicanlaw/law-docs": {
            "urls": [
                "git@github.com:oll-test-repos/mohicanlaw-law-docs.git"
            ],
            "custom": {
                "allow-unauthenticated-commits": true,
                "serve": "latest",
                "location_regex": "~/.*\\.pdf$"
            }
        },
        "mohicanlaw/law-xml": {
            "urls": [
                "git@github.com:oll-test-repos/mohicanlaw-law-xml.git"
            ],
            "custom": {
                "type": "xml",
                "allow-unauthenticated-commits": true,
                "serve": "latest",
                "serve-prefix": "_uncodified_xml"
            }
        },
        "mohicanlaw/law-html": {
            "urls": [
                "git@github.com:oll-test-repos/mohicanlaw-law-html.git"
            ],
            "custom": {
                "preview": "git@github.com:oll-test-repos/mohicanlaw-law-html-preview.git",
                "type": "html",
                "serve": "historical",
                "location_regex": "/"
            }
        },
        "mohicanlaw/law-xml-codified": {
            "urls": [
                "git@github.com:oll-test-repos/mohicanlaw-law-xml-codified.git"
            ],
            "custom": {
                "preview": "git@github.com:oll-test-repos/mohicanlaw-law-xml-codified-preview.git",
                "type": "xml-codified",
                "serve": "historical",
                "serve-prefix": "_xml"
            }
        },
        "mohicanlaw/law-static-assets": {
            "urls": [
                "git@github.com:oll-test-repos/mohicanlaw-law-static-assets.git"
            ],
            "custom": {
                "type": "static-assets",
                "allow-unauthenticated-commits": true,
                "serve": "latest",
                "location_regex": "~.+(_document|_reader).+$"
            }
        }
    }
}